<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RestApp.Login">
    <ContentPage.Content>
        <StackLayout BackgroundColor="Aqua">

            <Label BackgroundColor="AntiqueWhite" Text="Login" FontSize="20"/>

            <Label Text="Username"/>
            <Entry x:Name="entUsername" ></Entry>
            <Label Text="Password"/>
            <Entry x:Name="entPassword"></Entry>
            <Button Text="Sign in" Clicked="Logg"/>



        </StackLayout>
    </ContentPage.Content>
</ContentPage>